<workout_file>
    <author>Kevin Poulton / Powerhouse</author>
    <name>Mat Hayman Paris Roubaix 1</name>
    <description>This is a pretty full on effort that will give users an idea of the training sessions that were required for Mat Hayman to win Paris Roubaix.  If you find it too difficult, try to complete just one of the two blocks of intervals.

Authored by Keven Poulton</description>
    <sportType>bike</sportType>
    <tags>
        <tag name="INTERVALS"/>
        <tag name="ParisRoubaix"/>
    </tags>
    <workout>
        <Warmup Duration="300" PowerLow="0.39" PowerHigh="0.62"/>
        <SteadyState Duration="10" Power="2.78"/>
        <SteadyState Duration="60" Power="0.64"/>
        <SteadyState Duration="10" Power="2.78"/>
        <SteadyState Duration="60" Power="0.64"/>
        <SteadyState Duration="10" Power="2.78"/>
        <SteadyState Duration="180" Power="0.75"/>
        <SteadyState Duration="1200" Power="0.81">
            <textevent timeoffset="0" message="Cadence 5rpm above comfortable"/>
        </SteadyState>
        <SteadyState Duration="300" Power="0.5"/>
        <IntervalsT Repeat="6" OnDuration="120" OffDuration="20" OnPower="0.88" OffPower="1.4"/>
        <SteadyState Duration="60" Power="0.88"/>
        <SteadyState Duration="300" Power="0.5"/>
        <IntervalsT Repeat="6" OnDuration="120" OffDuration="20" OnPower="0.88" OffPower="1.4"/>
        <SteadyState Duration="60" Power="0.88"/>
        <Cooldown Duration="600" PowerLow="0.64" PowerHigh="0.25"/>
    </workout>
</workout_file>
